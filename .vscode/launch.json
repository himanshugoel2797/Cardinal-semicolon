{
        "version": "0.2.0",
        "configurations": [
        {
            "name": "(gdb) sign_exec",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/utils_build/sign_exec/sign_exec",
            "args": [ "m", "d", "d2", "0000", "0000", "FF", "README.md", "-o", "signed_sign_exec"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
        ]
    }